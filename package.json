{
  "name": "org.inovus.local-auth",
  "version": "0.4.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./dist/org.inovus.local-auth.css": "./dist/org.inovus.local-auth.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "npm run build && npm run verify",
    "build": "vite build",
    "verify": "npm pack --dry-run > npm-pack.txt",
    "dev": "vite",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "check": "biome check .",
    "check:fix": "biome check --write ."
  },
  "sideEffects": false,
  "author": "",
  "license": "MIT",
  "description": "",
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
    "@types/js-cookie": "^3.0.6",
    "@types/react": "^19.1.12",
    "lucide-react": "^0.542.0",
    "tailwindcss": "^4.0.0",
    "@tailwindcss/vite": "^4.0.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vite-plugin-dts": "^4.5.4"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "axios-auth-refresh": "^3.3.6",
    "js-cookie": "^3.0.5",
    "zod": "^4.1.4"
  },
  "peerDependencies": {
    "lucide-react": ">=0.542.0",
    "react": ">=18",
    "react-dom": ">=18",
    "react-router-dom": ">=6.0.0"
  }
}
